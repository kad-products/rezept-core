{
	"name": "@redwoodjs/starter",
	"version": "1.3.0",
	"description": "A bare-bones RedwoodSDK starter",
	"main": "index.js",
	"type": "module",
	"keywords": [],
	"author": "",
	"license": "MIT",
	"private": true,
	"scripts": {
		"build": "vite build",
		"dev": "npm run clean:vite && vite dev",
		"dev:init": "rw-scripts dev-init",
		"preview": "vite preview",
		"worker:run": "rw-scripts worker-run",
		"clean:vite": "rm -rf ./node_modules/.vite",
		"release": "rw-scripts ensure-deploy-env && npm run clean && npm run build && wrangler deploy",
		"generate": "rw-scripts ensure-env && wrangler types --include-runtime false",
		"biome:check": "biome check",
		"biome:fix": "biome check --write .",
		"type-check": "tsc",
		"prepare": "husky",
		"db:seed": "tsx scripts/seeding/main.ts",
		"db:reset-local": "tsx scripts/db-reset.ts",
		"db:load": "tsx scripts/db-load.ts",
		"db:migrate:new": "drizzle-kit generate",
		"db:migrate:dev": "wrangler d1 migrations apply rezept_core --local",
		"db:migrate:prod": "wrangler d1 migrations apply rezept_core --remote",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:run": "vitest run --coverage",
		"erd:generate": "drizzle-mermaid-generator --dialect sqlite --schema ./src/models/index.ts --relational > docs/data/current-erd.mmd",
		"erd:render": "npx -p @mermaid-js/mermaid-cli mmdc -i docs/data/current-erd.mmd -o docs/data/current-erd.png -t forest -w 3000 -H 2000"
	},
	"dependencies": {
		"@simplewebauthn/browser": "^13.2.2",
		"@simplewebauthn/server": "^13.2.2",
		"@tanstack/react-devtools": "^0.9.5",
		"@tanstack/react-form": "^1.28.0",
		"@tanstack/react-form-devtools": "^0.2.13",
		"classnames": "^2.5.1",
		"conventional-changelog-conventionalcommits": "^9.1.0",
		"dayjs": "^1.11.13",
		"drizzle-orm": "^0.45.1",
		"drizzle-zod": "^0.8.3",
		"i18n-iso-countries": "^7.14.0",
		"object-path": "^0.11.8",
		"react": "19.2.3",
		"react-dom": "19.2.3",
		"react-icons": "^5.5.0",
		"react-select": "^5.10.2",
		"react-server-dom-webpack": "19.2.3",
		"rwsdk": "1.0.0-beta.42",
		"ua-parser-js": "^2.0.8",
		"uuid": "^13.0.0",
		"zod": "^4.3.6"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.13",
		"@cloudflare/vite-plugin": "1.16.1",
		"@cloudflare/workers-types": "4.20251202.0",
		"@commitlint/cli": "^20.4.1",
		"@commitlint/config-conventional": "^20.4.1",
		"@faker-js/faker": "^10.1.0",
		"@libsql/client": "^0.17.0",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/commit-analyzer": "^13.0.1",
		"@semantic-release/git": "^10.0.1",
		"@semantic-release/release-notes-generator": "^14.1.0",
		"@types/better-sqlite3": "^7.6.13",
		"@types/node": "~24.5.2",
		"@types/object-path": "^0.11.4",
		"@types/papaparse": "^5.5.2",
		"@types/qs": "^6.14.0",
		"@types/react": "19.1.2",
		"@types/react-dom": "19.1.2",
		"@vitest/coverage-v8": "^4.0.18",
		"@vitest/ui": "^4.0.18",
		"chalk-template": "^1.1.2",
		"change-case": "^5.4.4",
		"drizzle-kit": "^0.31.8",
		"drizzle-mermaid-generator": "^0.0.5",
		"drizzle-seed": "^0.3.1",
		"husky": "^9.1.7",
		"less": "^4.3.0",
		"lint-staged": "^16.2.7",
		"papaparse": "^5.5.3",
		"tsx": "^4.21.0",
		"typescript": "5.8.3",
		"vite": "7.2.6",
		"vitest": "^4.0.18",
		"wrangler": "4.52.1"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild",
			"sharp",
			"workerd"
		]
	},
	"lint-staged": {
		"*": [
			"biome check --write --no-errors-on-unmatched --files-ignore-unknown=true"
		],
		"worker-configuration.d.ts": []
	},
	"packageManager": "pnpm@10.28.2+sha512.41872f037ad22f7348e3b1debbaf7e867cfd448f2726d9cf74c08f19507c31d2c8e7a11525b983febc2df640b5438dee6023ebb1f84ed43cc2d654d2bc326264"
}
