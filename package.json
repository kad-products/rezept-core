{
	"name": "@redwoodjs/starter",
	"version": "1.0.0",
	"description": "A bare-bones RedwoodSDK starter",
	"main": "index.js",
	"type": "module",
	"keywords": [],
	"author": "",
	"license": "MIT",
	"private": true,
	"scripts": {
		"build": "vite build",
		"dev": "npm run clean:vite && vite dev",
		"dev:init": "rw-scripts dev-init",
		"preview": "vite preview",
		"worker:run": "rw-scripts worker-run",
		"clean:vite": "rm -rf ./node_modules/.vite",
		"migrate:new": "drizzle-kit generate",
		"migrate:dev": "wrangler d1 migrations apply rezept_core --local",
		"migrate:prod": "wrangler d1 migrations apply rezept_core --remote",
		"release": "rw-scripts ensure-deploy-env && npm run clean && npm run build && wrangler deploy",
		"generate": "rw-scripts ensure-env && wrangler types --include-runtime false",
		"format": "biome format --write .",
		"lint": "biome lint --write .",
		"check": "biome check --write .",
		"type-check": "tsc",
		"prepare": "husky",
		"seed": "tsx scripts/seeding/main.ts",
		"db-reset": "tsx scripts/db-reset.ts"
	},
	"dependencies": {
		"@simplewebauthn/browser": "^13.2.2",
		"@simplewebauthn/server": "^13.2.2",
		"classnames": "^2.5.1",
		"dayjs": "^1.11.13",
		"drizzle-orm": "^0.45.1",
		"drizzle-zod": "^0.8.3",
		"react": "19.2.3",
		"react-dom": "19.2.3",
		"react-icons": "^5.5.0",
		"react-server-dom-webpack": "19.2.3",
		"rwsdk": "1.0.0-beta.42",
		"ua-parser-js": "^2.0.8",
		"uuid": "^13.0.0",
		"zod": "^4.3.6"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.13",
		"@cloudflare/vite-plugin": "1.16.1",
		"@cloudflare/workers-types": "4.20251202.0",
		"@faker-js/faker": "^10.1.0",
		"@libsql/client": "^0.17.0",
		"@types/better-sqlite3": "^7.6.13",
		"@types/node": "~24.5.2",
		"@types/react": "19.1.2",
		"@types/react-dom": "19.1.2",
		"drizzle-kit": "^0.31.8",
		"drizzle-seed": "^0.3.1",
		"husky": "^9.1.7",
		"less": "^4.3.0",
		"tsx": "^4.21.0",
		"typescript": "5.8.3",
		"vite": "7.2.6",
		"wrangler": "4.52.1"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild",
			"sharp",
			"workerd"
		]
	}
}
